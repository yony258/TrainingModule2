<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>trainingexample2</id>
	<name>Training Example 2 Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.trainingexample2</package>
	<author>Yonatan</author>
	<description>
		Just a small training example
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/trainingexample2/update.rdf</updateURL>
	<require_version>1.9.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.trainingexample2.TrainingExample2Activator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.trainingexample2.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		TrainingExample2.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with trainingexample2. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

